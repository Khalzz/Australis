config_version=5

[application]

config/name="Australis"
run/main_scene="res://Scenes/SplashScreen.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
boot_splash/bg_color=Color(0.109804, 0.113725, 0.145098, 1)
boot_splash/show_image=false
boot_splash/image="res://Assets/Credits/Developed by Rodrigo Seguel Find more at rseguel.com.png"
boot_splash/fullsize=false
config/icon="res://Assets/Sprites/icon.png"

[autoload]

Enums="*res://Scripts/GLOBAL/enums.gd"
Items="*res://Scripts/GLOBAL/items.gd"
Save="*res://Scripts/GLOBAL/save.gd"
SceneManagement="*res://Scripts/GLOBAL/scene_management.gd"

[display]

